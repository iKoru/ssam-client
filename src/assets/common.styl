html
  overflow-y: auto
.cursor-pointer
  cursor:pointer
// vuetify-related
.v-btn.plain:hover:before,
.v-btn.plain:focus:before,
.v-btn.plain.v-btn--active:hover:before
  background-color: transparent
  
.v-text-field .v-text-field__details
  margin-top:4px

.v-text-field.dense .v-label, .v-select.dense .v-label
  font-size: 14px

.v-text-field.dense input, .v-select.dense input
  max-height: 28px
  padding-bottom: 0px
